<script>
  let cart = JSON.parse(localStorage.getItem('cart')) || [];

  if (cart.length > 0 && '<% displayName %>') {
    document.getElementById('checkout-btn').classList.remove('hidden');
  }
  if (cart.length > 0 && !'<% displayName %>') {
    document.getElementById('checkout-button-guest').classList.remove('hidden');
  }

  // Get the main image element
  const mainImage = document.getElementById("main-image");
  function showImage(imagePathSelected){
    mainImage.src = imagePathSelected;
  }

  document.getElementById('add-item-to-cart-button').addEventListener('click', function(){ 
      processAddItemsToCart();
  });

function processAddItemsToCart() { 
  cart = JSON.parse(localStorage.getItem('cart')) || [];
  let projectPrice = 0;
  let identifier = cart.length;
  let quantity = 1;
  let priceAddOns = 0;
  const projectId = '<%= project._id %>';
  let notes = '';
    notes = null;

  const projectToAdd = {
    projectId: projectId,
    projectName: '<%= project.title %>',
    projectImagePath: '<%= project.imgUrl %>',
    category: '<%= project.category %>',
    projectPrice: '<%= project.price %>',
    priceAddOns: priceAddOns,
    identifier: identifier,
    quantity: quantity,
    customImages: `<%- project.customImages %>`,
    notes: notes
  };
  cart.push(projectToAdd);
  localStorage.setItem('cart', JSON.stringify(cart));

  //For Form Submission: fill inputs
  document.getElementById('projectId-input').value = '<%= project._id %>';
  document.getElementById('projectName-input').value = '<%= project.title %>';
  document.getElementById('projectImagePath-input').value = '<%= project.imgUrl %>';
  document.getElementById('projectCategory-input').value = '<%= project.category %>';
  document.getElementById('projectPrice-input').value = projectPrice;
  document.getElementById('priceAddOns-input').value = priceAddOns;
  document.getElementById('notes-input').value = notes;
  document.getElementById('customImagesEditAreas-input').value = `<%- project.customImagesEditAreas %>`;
  document.getElementById('customImages-input').value = `<%- project.customImages %>`;
  
  
  
  
  document.getElementById('success-added-cart-item-label').style.display = 'block';
  

  setTimeout(() => {
  window.location.reload();
}, 500);

}


</script>
