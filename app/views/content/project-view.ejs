          <script>
            function toggleDropdown(disclosureId, svgprojectClosedId, svgprojectOpenId) {
              const disclosure = document.getElementById(disclosureId);
              const svgprojectClosed = document.getElementById(svgprojectClosedId);
              const svgprojectOpen = document.getElementById(svgprojectOpenId);
              const isExpanded = disclosure.style.display === 'block' || disclosure.style.display === '';
              disclosure.style.display = isExpanded ? 'none' : 'block';
              if (!isExpanded) {
                svgprojectClosed.classList.add('hidden');
                svgprojectOpen.classList.remove('hidden');
              } else {
                svgprojectClosed.classList.remove('hidden');
                svgprojectOpen.classList.add('hidden');
              }
            }
